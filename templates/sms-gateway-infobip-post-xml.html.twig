XML=
<SMS>
  <authentication>
    <username>{{ body.username }}</username>
    <password>{{ body.password }}</password>
  </authentication>
  <message>
    <sender>{{ body.sender }}</sender>
    <text>{{ body.SMSText }}</text>
    <flash>{{ body.IsFlash }}</flash>
    <type>{{ body.Type }}</type>
    <pushurl>{{ body.pushurl }}</pushurl>
    <nopush>{{ body.nopush }}</nopush>
    <recipients>
      {{ body.GSM }}
    </recipients>
  </message>
</SMS>
